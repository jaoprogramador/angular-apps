{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://api.chucknorris.io/jokes/random"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/media/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
      
    }
  ],  
  "dataGroups": [
    {
      "name": "Chistes chuck norris",
      "urls": [
        "https://api.chucknorris.io/jokes/random"
        ],        
        "cacheConfig": {
          "strategy": "freshness",
          "maxAge": "1h",
          "maxSize": 20,
          "timeout": "4s"
        }
    }
  ]
}
